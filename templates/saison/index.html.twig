{% extends 'baseadmin.html.twig' %}

{% block title %}Saison index{% endblock %}

{% block body %}
    <h1>Saison index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Idsaison</th>
                <th>Nomsaison</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for saison in saisons %}
            <tr>
                <td>{{ saison.idsaison }}</td>
                <td>{{ saison.nomsaison }}</td>
                <td>
                    <a href="{{ path('app_saison_show', {'idsaison': saison.idsaison}) }}">show</a>
                    <a href="{{ path('app_saison_edit', {'idsaison': saison.idsaison}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_saison_new') }}">Create new</a>
{% endblock %}
